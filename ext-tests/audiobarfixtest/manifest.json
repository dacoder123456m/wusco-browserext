{
  "manifest_version": 2,
  "name": "Custom Volume Control",
  "version": "1.0",
  "description": "Custom volume control UI",
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  }
}
